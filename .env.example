# PrivateGPT Environment Variables
# Copy this file to .env and update with your values

# Database
DATABASE_URL=postgresql://privategpt:secret@db:5432/privategpt
REDIS_URL=redis://redis:6379/0
WEAVIATE_URL=http://weaviate:8080

# Authentication
KEYCLOAK_URL=http://keycloak:8080
KEYCLOAK_REALM=privategpt
KEYCLOAK_CLIENT_ID=privategpt-api
KEYCLOAK_CLIENT_SECRET=your-client-secret
KEYCLOAK_ADMIN_PASSWORD=admin123

# Default Admin User
DEFAULT_ADMIN_EMAIL=admin@admin.com
DEFAULT_ADMIN_USERNAME=admin@admin.com
DEFAULT_ADMIN_PASSWORD=admin
DEFAULT_ADMIN_FIRST_NAME=Admin
DEFAULT_ADMIN_LAST_NAME=User

# Service URLs
GATEWAY_URL=http://gateway-service:8000
RAG_SERVICE_URL=http://rag-service:8000
LLM_SERVICE_URL=http://llm-service:8000
MCP_SERVICE_URL=http://mcp-service:8000
OLLAMA_URL=http://ollama:11434

# LLM Configuration
LLM_PROVIDER=ollama
LLM_DEFAULT_MODEL=
EMBED_MODEL=BAAI/bge-small-en-v1.5

# API Keys (REQUIRED for external providers)
# NEVER commit these to version control!
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Optional: Override specific settings
# OLLAMA_MODEL=llama3.2
# ENABLE_THINKING_MODE=true
# MCP_ENABLED=true

# N8N Workflow Password
N8N_PASSWORD=changeme